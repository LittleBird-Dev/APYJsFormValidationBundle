function Time(value, params)
{
    if (isNotDefined(value)) {
        return true;
    }

    value = String(value);

    var pattern = /^([0-1][0-9]|2[0-3]):([0-5][0-9]):([0-5][0-9])$/;

    if ( pattern.test(value) ) {
        return true;
    }

    return getComputeMessage(params.message);
}